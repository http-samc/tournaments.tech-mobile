{
  "expo": {
    "name": "tournaments.tech",
    "slug": "tournaments-tech",
    "description": "Reliable and accurate rankings for the Public Forum national circuit!",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#121212"
    },
